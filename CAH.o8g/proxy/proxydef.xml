<?xml version="1.0" encoding="utf-8" ?>
<templates>
  <blocks>
    <block type="text" id="qtext">
      <location x="20" y="20" />
      <text color="#FFFFFF" size="35" />
      <wordwrap height="400" width="335" />
    </block>
    <block type="text" id="atext">
      <location x="20" y="20" />
      <text color="#000000" size="35" />
      <wordwrap height="400" width="335" />
    </block>
  </blocks>

  <template src="proxy/w.png" default="True">
    <matches>
      <match name="Type" value="A" />
    </matches>
    <textblocks>
      <link block="atext">
        <property name="Name" />
      </link>
    </textblocks>
  </template>
  <template src="proxy/b.png">
